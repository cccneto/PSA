@article{ WOS:000268492100013,
Author = {Igari, Alexandre Toshiro and Tambosi, Leandro Reverberi and Pivello,
   Vania Regina},
Title = {{Agribusiness Opportunity Costs and Environmental Legal Protection:
   Investigating Trade-Off on Hotspot Preservation in the State of So
   Paulo, Brazil}},
Journal = {{ENVIRONMENTAL MANAGEMENT}},
Year = {{2009}},
Volume = {{44}},
Number = {{2}},
Pages = {{346-355}},
Month = {{AUG}},
Abstract = {{Prior to deforestation, So Paulo State had 79,000 km(2) covered by
   Cerrado (Brazilian savanna) physiognomies, but today less than 8.5\% of
   this biodiversity hotspot remains, mostly in private lands. The global
   demand for agricultural goods has imposed strong pressure on natural
   areas, and the economic decisions of agribusiness managers are crucial
   to the fate of Cerrado domain remaining areas (CDRA) in Brazil. Our aim
   was to investigate the effectiveness of Brazilian private protected
   areas policy, and to propose a feasible alternative to promote CDRA
   protection. This article assessed the main agribusiness opportunity
   costs for natural areas preservation: the land use profitability and the
   arable land price. The CDRA percentage and the opportunity costs were
   estimated for 349 municipal districts of So Paulo State through
   secondary spatial data and profitability values of 38 main agricultural
   products. We found that Brazilian private protected areas policy fails
   to preserve CDRA, although the values of non-compliance fines were
   higher than average opportunity costs. The scenario with very
   restrictive laws on private protected areas and historical high interest
   rates allowed us to conceive a feasible cross compliance proposal to
   improve environmental and agricultural policies.}},
