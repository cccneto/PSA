
@article{ WOS:000719530200002,
Author = {Lopes, Tarcio Rocha and Folegatti, Marcos Vinicius and Zolin, Cornelio
   Alberto and Moura, Larissa Bretas and Oliveira, Rubmara Ketzer and
   Fraga, Luciano Sobral and Duarte, Sergio Nascimento and Almeida Santos,
