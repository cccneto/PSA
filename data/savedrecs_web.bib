@article{ WOS:000391437200011,
Author = {Cunha Libanio, Paulo Augusto},
Title = {{The use of goal-oriented strategies for water pollution control in
   Brazil}},
Journal = {{ENGENHARIA SANITARIA E AMBIENTAL}},
Year = {{2016}},
Volume = {{21}},
Number = {{4}},
Pages = {{731-738}},
Month = {{OCT-DEC}},
Abstract = {{This paper presents an overview about the use of goal-oriented
   strategies, such as output-based aid (OBA) and payment for environmental
   services (PES), for water pollution abatement and control in urban and
   rural areas throughout the country. Thus, considering the goal of
   implementing IWRM, two nation-wide experiences are reviewed: the River
   Basin Clean-Up Program (PRODES) and the Water Producer Program, both run
   by the National Water Agency (ANA), the former initiated over a decade
   ago. They illustrate successful ways to bring public authorities and
   water users together, in this case, sanitation services and landowners,
   respectively. Finally, it is discussed the applicability and
   perspectives for carrying out new goal-oriented initiatives aimed at
   IWRM in Brazil.}},
