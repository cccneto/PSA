@article{ WOS:000398572000002,
Author = {Angelsen, Arild},
Title = {{REDD plus as Result-based Aid: General Lessons and Bilateral Agreements
   of Norway}},
Journal = {{REVIEW OF DEVELOPMENT ECONOMICS}},
