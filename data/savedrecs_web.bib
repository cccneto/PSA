@article{ WOS:000669366700014,
Author = {Campos, Rosiany Possati and Nunes de Oliveira, Ricardo Castro and Verol,
   Aline Pires and Haddad, Assed Naked and Miguez, Marcelo Gomes},
Title = {{Payment for environmental services for flood control analysis and method
   of economic viability}},
