@article{ WOS:000506242300003,
Author = {Greenleaf, Maron},
Title = {{Rubber and Carbon: Opportunity Costs, Incentives and Ecosystem Services
   in Acre, Brazil}},
Journal = {{DEVELOPMENT AND CHANGE}},
