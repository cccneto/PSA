@ARTICLE{Begossi201125,
author={Begossi, A. and May, P.H. and Lopes, P.F. and Oliveira, L.E.C. and da Vinha, V. and Silvano, R.A.M.},
title={Compensation for environmental services from artisanal fisheries in SE Brazil: Policy and technical strategies},
