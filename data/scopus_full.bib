@ARTICLE{Delacote2014508,
author={Delacote, P. and Palmer, C. and Bakkegaard, R.K. and Thorsen, B.J.},
title={Unveiling information on opportunity costs in REDD: Who obtains the surplus when policy objectives differ?},
journal={Resource and Energy Economics},
year={2014},
volume={36},
number={2},
pages={508-527},
doi={10.1016/j.reseneeco.2013.07.002},
note={cited By 22},
