@ARTICLE{Ribeiro20201,
author={Ribeiro, S.C. and Galia Selaya, N. and Perz, S.G. and Brown, F. and Schmidt, F.A. and Silva, R.C. and Lima, F.},
title={Aligning conservation and development goals with rural community priorities: Capacity building for forest health monitoring in an extractive reserve in Brazil},
journal={Ecology and Society},
year={2020},
volume={25},
number={3},
pages={1-13},
doi={10.5751/ES-11665-250305},
art_number={5},
note={cited By 1},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85089369773&doi=10.5751%2fES-11665-250305&partnerID=40&md5=b20623bdcfd6a66a9554b7d4acc2f0c1},
affiliation={Centro de Ciências Biológicas e da Natureza, Universidade Federal do Acre, Acre, Brazil; Santa Cruz, Bolivia; University of Florida, United States; Woods Hole Research Center, United States; Federal University of Acre, Brazil; Associação SOS Amazônia, Brazil; Instituto Federal de Educação, Ciência e Tecnologia do Acre, Brazil; SOS Amazônia, Brazil},
abstract={Rural communities are important stewards of forests that provide valuable ecological services. This makes them vital allies to outside organizations seeking to support conservation and development initiatives. However, rural communities also have priorities and needs that may not align with the goals of conservation and development projects. This makes effective engagement of communities by outside organizations an important challenge. When rural communities lose traditional livelihood options, they prioritize economic benefits as a condition for participating in projects, and they exhibit “project fatigue” when many projects are imposed on their time. We reflect on our experience in seeking to align the goals of a conservation and development project with community priorities in the Chico Mendes Extractive Reserve in Acre, Brazil. Our project featured capacity building for monitoring forest health to foster participation in payments for ecosystem services programs. Although we pursued prior consultations and designed participatory activities, and although we combined knowledge transmission with skills training, participation declined. That prompted our team to consult with community members, which generated important insights about expectations of immediate economic remuneration, community political cultures, communicative practices, and differences among local constituencies. These insights motivated adaptation of our practices via several strategies, such as focusing on young adults and holding workshops as part of community assemblies, which aligned project goals with the priorities of community members to improve project outcomes. © 2020 by the author(s). Published here under license by the Resilience Alliance.},
author_keywords={Amazon;  Capacity building;  Community;  Conservation;  Development;  Environmental monitoring;  Forests},
keywords={conservation planning;  development project;  ecosystem service;  forest management;  health monitoring;  local participation;  nature reserve;  rural development, Acre [Brazil];  Brazil},
funding_details={United States Agency for International DevelopmentUnited States Agency for International Development, USAID},
funding_details={Instituto Chico Mendes de Conservação da BiodiversidadeInstituto Chico Mendes de Conservação da Biodiversidade, ICMBio},
