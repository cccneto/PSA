@ARTICLE{DeMartino201768,
author={De Martino, S. and Kondylis, F. and Zwager, A.},
title={Protecting the Environment: For Love or Money? The Role of Motivation and Incentives in Shaping Demand for Payments for Environmental Services Programs},
journal={Public Finance Review},
year={2017},
volume={45},
number={1},
pages={68-96},
doi={10.1177/1091142115604352},
note={cited By 8},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85002339015&doi=10.1177%2f1091142115604352&partnerID=40&md5=c9e89537d2d572496f718cf02682bf9f},
affiliation={Department of Economics, University of Sussex, Falmer, United Kingdom; Development Research Group, World Bank, Washington, DC, United States},
abstract={The role of extrinsic versus intrinsic motivation in environmental decisions remain unresolved. We exploit data from a lab-in-the-field experiment to analyze the role of extrinsic and intrinsic incentives in shaping individual demand for a payment for environmental services (PES) program in São Paulo, Brazil. The lab-in-the-field experiment is a theoretical incentive program that offers annual payments to landholders in vulnerable watersheds for either conserving and/or restoring trees surrounding springs on their land to preserve and improve local water quality. Our findings suggest that, in contrast with predictions from rational choice theory, individuals’ responses to incentives are not monotonic. Landholders who took part in our lab-in-the-field experiment were randomly assigned to four offer levels and asked a double-bounded contingent valuation question to elicit a willingness to accept value. Landholders were less likely to accept the higher offers compared to the lowest offers. Given that the rational choice model fails to fully account for the role of incentives in shaping demand for PES, we then look at the interaction of the randomized incentive offers and individuals’ initial intrinsic motivations. We find that, while high monetary incentives crowd in demand of progovernment landholders, they crowd out demand of proenvironment and prosocial landholders. Overall, we find much evidence of heterogeneous responses. © 2015, © The Author(s) 2015.},
author_keywords={extrinsic incentives;  field experiment;  intrinsic motivation;  payment for environmental services},
references={Arriagada, R., (2009) Payments for Environmental Services and Their Impact on Forest Transition in Costa Rica, , Working Papers, Latin American and Caribbean Environmental Economics Program, Turrialba, Costa Rica; Benabou, R., Tirole, J., Intrinsic and extrinsic motivation (2003) The Review of Economic Studies, 70, pp. 489-520; Deci, E.L., Ryan, R.M., (1985) Intrinsic Motivation and Self-determination in Human Behavior, , New York, Plenum; De Martino, S., Kondylis, F., Pagiola, S., Zwager, A., (2015) Do They Say As They Do? Stated versus Revealed Preferences and Take Up in an Incentives for Conservation Program, , PES Learning Paper No. 2015 -1, Washington, DC, World Bank; Eckel, C., Grossman, P.J., Altruism in Anonymous Dictator Games (1996) Games and Economic Behavior, 16, pp. 181-191; Frey, B.S., Tertium Datur: Pricing, Regulating and Intrinsic Motivation (1992) KYKLOS, 45, pp. 161-184; Gneezy, U., Rustichini, A., Pay Enough or Don’t Pay At All (2000) The Quarterly Journal of Economics, 115, pp. 791-810; Heyman, J., Ariely, D., Effort for Payment. A Tale of Two Markets (2004) Psychological Science, 15, pp. 787-793; Kerr, J., Vardhan, M., Jindal, R., Prosocial Behavior and Incentives: Evidence from Field Experiments in Rural Mexico and Tanzania (2012) Ecological Economics, 73, pp. 220-227; Lepper, M.R., Ed Greene, D., (1978) The Hidden Costs of Reward: New Perspectives on the Psychology of Human Motivation, , Hillsdale, NJ, Lawrence Erlbaum; Lopez-Feldman, A., (2012) Introduction to Contingent Valuation Using Stata, , MPRA Paper 41018, University Library of Munich, Munich, Germany; Meier, S., A Survey of Economic Theories and Field Evidence on Pro-social Behavior (2007) Economics and Psychology. A Promising New Cross-disciplinary Field, pp. 51-87. , Frey Bruno S., Stutzer Alois, (eds), Cambridge, MA, MIT Press,.” In, edited by; Pagiola, S., Rios, A., Arcenas, A., Poor Household Participation in Payments for Environmental Services: Lessons from the Silv