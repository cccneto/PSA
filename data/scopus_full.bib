@ARTICLE{Dias2021,
author={Dias, A.L.S. and Lange, L.C. and Magalhães, A.S.},
title={Application of a ‘Recycling Exchange’ instrument to compensate waste pickers in Brazil via a first payment for urban environmental services programme},
journal={Waste Management and Research},
